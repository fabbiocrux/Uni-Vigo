@Article{Sachs1993,
  Author = {Sachs, Emanuel and Cima, Michael and Cornie, James and Brancazio, David and Bredt, Jim and Curodeau, Alain and Fan, Tailin and Khanuja, Satbir and Lauder, Alan and Lee, John and Michaels, Steve},
  Journal = {CIRP Ann. - Manuf. Technol.},

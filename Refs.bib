@Article{Rani2019,
  Author = {Rani, Monika and Marchesi, Claudio and Federici, Stefania and Rovelli, Gianluca and Alessandri, Ivano and Vassalini, Irene and Ducoli, Serena and Borgese, Laura and Zacco, Annalisa and Bilo, Fabjola and Bontempi, Elza and Depero, Laura E.},

  Keywords = {Animal,Animals,Biocompatible,Biocompatible: chemistry,Biocompatible: therapeutic use,Bone Substitutes,Bone Substitutes: chemistry,Bone Substitutes: therapeutic use,Bone and Bones,Bone and Bones: chemistry,Cell Culture Techniques,Cell Culture Techniques: methods,Cell Separation,Cell Separation: methods,Coated Materials,Durapatite,Durapatite: chemistry,Durapatite: therapeutic use,Elasticity,Humans,Mice,Models,Nude,Osteoblasts,Osteoblasts: transplantation,Polyesters,Polyesters: chemistry,Polyesters: therapeutic use,Tissue Engineering,Tissue Engineering: methods,Weight-Bearing},

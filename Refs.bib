  File = {:Users/fabio/Library/Application Support/Mendeley Desktop/Downloaded/Villalpando, Eiliat, Urbanic - 2014 - An optimization approach for components built by fused deposition modeling with parametric interna.pdf:pdf},
  Issn = {22128271},
  Keywords = {Additive manufacturing,Fused deposition modelling,Genetic algorithm,Internal parametric structures,Optimization},
  Isbn = {22128271}
}



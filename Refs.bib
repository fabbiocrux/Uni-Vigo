  Issn = {09596526},
  Keywords = {Circular economy,Recycling,Resource efficiency,Reuse,Sustainability,Zero waste},
  Isbn = {0959-6526},
  Pmid = {24910141},
  Archiveprefix = {arXiv},
  Arxivid = {arXiv:1011.1669v3},
  Eprint = {arXiv:1011.1669v3}
}



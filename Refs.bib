@Article{Roy2020,
  Author = {Roy, Sanjeeb and Das, Miki and Ali, Syed Mithun and Raihan, Ahmed Shoyeb and Paul, Sanjoy Kumar and Kabir, Golam},
  Journal = {J. Clean. Prod.},
  Month = {apr},
  Pages = {121389},

@Article{Fonseca2018,
  Author = {Fonseca, Juan D and Camargo, Mauricio and Commenge, Jean Marc and Falk, Laurent and Gil, Iv{\'{a}}n D.},
  Journal = {Int. J. Hydrogen Energy},
